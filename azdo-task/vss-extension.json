    "contributions": [
        {
            "id": "dev-container-build-run",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "DevContainerBuildRun"
            }
        }
    ]
}