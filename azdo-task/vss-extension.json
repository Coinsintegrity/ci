{
    "manifestVersion": 1,
    "id": "devcontainer-build-run",
    "name": "Dev Container Build & Run Task",
    "version": "0.0.1",
    "publisher": "stuartleeks",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Extension for building and running VS Code Dev Containers in Azure DevOps Pipelines",
    "categories": [
        "Azure Pipelines"
